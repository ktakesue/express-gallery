<h1 id="title">welcome to ur gallery</h1>

<form method="POST" action="/gallery">
    <button type="submit">Create New Image</button>
</form>

<br>
<a id="back" href="/">GO BACK BITCH</a>

<div id="gallery">
    {{#each gallery}}
    <h1 id="author">{{author}}</h1>
    <h2>
        <img id="image" src="{{link}}">
    </h2>
    <h3 id="description">{{description}}</h3>
    {{/each}}
</div>